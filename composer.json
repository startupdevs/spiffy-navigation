        "zendframework/zend-mvc": "4.0.x-dev",
        "zendframework/zend-permissions-rbac": "^3.0.2",
        "zendframework/zend-servicemanager": "^3.4.0",
        "zendframework/zend-stdlib": "^3.2.1",
        "zendframework/zend-view": "2.12.x-dev"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
