{
    "name": "spiffy/spiffy-navigation",
    "description": "Fork of spiffy/spiffy-navigation for Zend Framework 3",
    "type": "library",
    "keywords": [
        "module",
        "zf3"
    ],
    "homepage": "https://github.com/startupdevs/spiffy-navigation/",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        }
    ],

    "require": {
        "php": ">=5.6",
        "laminas/laminas-cli": "1.0.0",
        "laminas/laminas-eventmanager": ">=2.0.0",
        "laminas/laminas-form": ">=2.0.0",
        "laminas/laminas-modulemanager": ">=2.0.0",
        "laminas/laminas-mvc": ">=2.0.0",
        "laminas/laminas-permissions-acl": ">=2.0.0",
        "laminas/laminas-permissions-rbac": ">=2.0.0",
        "laminas/laminas-serializer": ">=2.0.0",
        "laminas/laminas-servicemanager": ">=2.0.0",
        "laminas/laminas-stdlib": ">=2.0.0",
        "laminas/laminas-uri": ">=2.0.0",
        "laminas/laminas-view": ">=2.0.0"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": ">=2.0.0",
        "laminas/laminas-cli": "1.0.0"
    },

    "autoload": {
        "psr-0": {
            "SpiffyNavigation": "src/",
            "SpiffyNavigationTest": "test/"
        }
    }
}
